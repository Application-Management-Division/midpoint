<itemsCorrelator
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3">
    <name>by-name-levenshtein</name>
    <item>
        <path>givenName</path>
        <search>
            <fuzzy>
                <levenshtein>
                    <threshold>1</threshold>
                </levenshtein>
            </fuzzy>
        </search>
    </item>
    <item>
        <path>familyName</path>
        <search>
            <fuzzy>
                <similarity>
                    <threshold>0.6</threshold>
                </similarity>
            </fuzzy>
        </search>
    </item>
</itemsCorrelator>
