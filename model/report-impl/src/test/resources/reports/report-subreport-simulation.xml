<report oid="729ce99c-8fcb-47d4-9063-19fa850870d2" xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3">
    <name>Object Collection: simulation</name>
    <objectCollection>
        <view>
            <column>
                <name>oid</name>
                <display>
                    <label>Oid</label>
                </display>
                <export>
                    <expression>
                        <script>
                            <code>
                                return object.oid
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
            <column>
                <name>name</name>
                <!--<path>$object/name</path>-->
                <display>
                    <label>Name</label>
                </display>
                <previousColumn>oid</previousColumn>
                <export>
                    <expression>
                        <script>
                            <code>
                                return object.name.orig
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
            <column>
                <name>Assignment</name>
                <!--<path>$assignment/targetRef</path>-->
                <display>
                    <label>Assignment</label>
                </display>
                <previousColumn>name</previousColumn>
                <export>
                    <expression>
                        <script>
                            <code>
                                return assignment.targetRef
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
            <type>UserType</type>
        </view>
        <useOnlyReportView>true</useOnlyReportView>
        <subreport>
            <name>assignment</name>
            <type>AssignmentType</type>
            <expression>
                <script>
                    <code>
                        import com.evolveum.midpoint.xml.ns._public.common.common_3.UserType

                        def object = object as UserType

                        return object.getAssignment()
                    </code>
                </script>
            </expression>
            <asRow>true</asRow>
        </subreport>
    </objectCollection>
</report>
