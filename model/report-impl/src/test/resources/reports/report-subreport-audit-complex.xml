<report oid="44026fc7-c73d-4210-91c3-e5d10391c02b" xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3" xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3">
    <name>Object Collection: subreport audit</name>
    <objectCollection>
        <collection>
            <filter>
                <q:and>
                    <q:equal>
                        <q:path>eventStage</q:path>
                        <q:value>EXECUTION</q:value>
                    </q:equal>
                    <q:equal>
                        <q:path>eventType</q:path>
                        <q:value>MODIFY_OBJECT</q:value>
                    </q:equal>
                </q:and>
            </filter>
        </collection>
        <view>
            <column>
                <name>timestamp</name>
                <path>timestamp</path>
                <display>
                    <label>Timestamp</label>
                </display>
                <export>
                    <expression>
                        <script>
                            <code>
                                return object.timestamp
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
            <column>
                <name>timestamp</name>
                <path>timestamp</path>
                <display>
                    <label>Timestamp</label>
                </display>
            </column>
            <column>
                <name>targetRef</name>
                <path>targetRef</path>
                <display>
                    <label>Target</label>
                </display>
                <previousColumn>timestamp</previousColumn>
            </column>
            <column>
                <name>delta</name>
                <display>
                    <label>Object delta</label>
                </display>
                <previousColumn>targetRef</previousColumn>
                <export>
                    <expression>
                        <script>
                            <code>
                                import com.evolveum.prism.xml.ns._public.types_3.ObjectDeltaType

                                def delta = delta as ObjectDeltaType

                                return delta.changeType
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
            <column>
                <name>delta</name>
                <display>
                    <label>Object delta</label>
                </display>
                <previousColumn>targetRef</previousColumn>
                <export>
                    <expression>
                        <script>
                            <code>
                                return delta
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
            <type>AuditEventRecordType</type>
        </view>
        <useOnlyReportView>true</useOnlyReportView>
        <subreport>
            <name>deltaOperation</name>
            <expression>
                <script>
                    <code>
                        import com.evolveum.midpoint.xml.ns._public.common.audit_3.AuditEventRecordType

                        def object = object as AuditEventRecordType

                        return object.delta()
                    </code>
                </script>
            </expression>
            <order>10</order>
            <asRow>true</asRow>
        </subreport>
        <subreport>
            <name>delta</name>
            <expression>
                <script>
                    <code>
                        import com.evolveum.midpoint.xml.ns._public.common.audit_3.AuditEventRecordType
                        import com.evolveum.midpoint.xml.ns._public.common.common_3.ObjectDeltaOperationType

                        def object = object as AuditEventRecordType
                        def deltaOperation = deltaOperation as ObjectDeltaOperationType

                        return deltaOperation.objectDelta
                    </code>
                </script>
            </expression>
            <order>20</order>
            <asRow>true</asRow>
        </subreport>
        <subreport>
            <name>itemDelta</name>
            <expression>
                <script>
                    <code>
                        import com.evolveum.midpoint.xml.ns._public.common.audit_3.AuditEventRecordType
                        import com.evolveum.midpoint.xml.ns._public.common.common_3.ObjectDeltaOperationType
                        import com.evolveum.prism.xml.ns._public.types_3.ObjectDeltaType

                        def object = object as AuditEventRecordType
                        def deltaOperation = deltaOperation as ObjectDeltaOperationType
                        def delta = delta as ObjectDeltaType

                        return delta.itemDelta
                    </code>
                </script>
            </expression>
            <order>30</order>
            <asRow>true</asRow>
        </subreport>
    </objectCollection>
</report>
