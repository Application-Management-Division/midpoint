<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="integration" parallel="false" verbose="1">
    <listeners>
        <listener class-name="com.evolveum.midpoint.tools.testng.AlphabeticalMethodInterceptor"/>
    </listeners>
    <test name="Repo common" preserve-order="true" parallel="false" verbose="10">
        <classes>
            <class name="com.evolveum.midpoint.repo.common.commandline.TestCommandLine"/>
            <class name="com.evolveum.midpoint.repo.common.tasks.TestActivities"/>
            <class name="com.evolveum.midpoint.repo.common.tasks.TestBucketManagement"/>
            <class name="com.evolveum.midpoint.repo.common.tasks.TestWorkDistribution"/>
        </classes>
    </test>
</suite>
